
<h1><%= @restaurant.name %></h1>
<p><%= @restaurant.address %></p>
<p><%= @restaurant.phone_number %></p>
<p><%= @restaurant.category %></p>
<h2><%=" Reviews :" %>
<% @restaurant.reviews.each do |review| %>
  <p><%= review.content %></p>
<% end %>

<h2><%= link_to 'Edit', edit_restaurant_path(@restaurant) %></h2>
<h2><%= link_to 'Delete', restaurant_path(@restaurant) %></h2>
<h2><%= link_to 'Add review', new_restaurant_review_path(@restaurant) %></h2>


